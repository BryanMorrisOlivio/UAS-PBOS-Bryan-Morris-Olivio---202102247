a = int(input("A : "));
b = int(input("B : "));
class kalku:
 def  __init__(self,a,b):
        self.a = a
        self.b = b
 def tambah(self,a,b):
        return self.a+self.b
 def kurang(self,a,b):
        return self.a-self.b
 def bagi(self,a,b):
        return self.a//self.b
 def kali(self,a,b):
        return self.a*self.b
cal = kalku(a,b)
print("Tambah : " , cal.tambah(a,b))
print("Kurang : " , cal.kurang(a,b))
print("Kali : " , cal.kali(a,b))
print("Bagi : " , cal.bagi(a,b))
